void setup() {
    pinMode(D4, OUTPUT);
    pinMode(D5, OUTPUT);
    pinMode(D1, OUTPUT);
    pinMode(D3, OUTPUT);
}

void loop() {
    
    digitalWrite(D3, HIGH);
    digitalWrite(D1, HIGH);
    delay(7000);
    digitalWrite(D3, LOW);
    digitalWrite(D1, LOW);
    delay(7000);
    
    digitalWrite(D5, HIGH);
    digitalWrite(D4, HIGH);
    delay(7000);
    digitalWrite(D5, LOW);
    digitalWrite(D4, LOW);
    delay(7000);
    
}
